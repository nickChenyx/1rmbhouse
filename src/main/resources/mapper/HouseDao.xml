<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rmbhouse.dao.HouseDao">
    <resultMap id="house" type="com.rmbhouse.entity.House">
        <id property="h_id" column="h_id"></id>
        <result property="tag" column="tag"></result>
        <result property="price" column="price"></result>
        <result property="communityName" column="community_name"></result>
        <result property="balcony" column="balcony"></result>
        <result property="bathroom" column="bathroom"></result>
        <result property="kitchen" column="kitchen"></result>
        <result property="room" column="room"></result>
        <result property="hall" column="hall"></result>
        <result property="detail" column="detail"></result>
        <result property="rentType" column="rent_type"></result>
        <result property="subway" column="subway"></result>
        <result property="who" column="who"></result>
    </resultMap>
    <select id="queryByPrice" resultMap="house">
        SELECT * FROM house WHERE price> #{lowPrice} AND price < #{highPrice}
    </select>



</mapper>